// This programs finds the factorial of a number provided by the user. 

#include <stdio.h>

int main(void) {
  int num=0, factorial=1, i=0;

  printf("Please enter an integer.\n");
  scanf("%d", &num);
  for (i=1; i<num+1; i++) {
    factorial=factorial*i;
  }
  printf("The factorial is %d", factorial);
  return 0;
}
